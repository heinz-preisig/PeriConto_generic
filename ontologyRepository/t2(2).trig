@prefix newClass: <http://example.org/newClass> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix root: <http://example.org/root> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

newClass: {
    newClass: a rdfs:Class .

    <http://example.org/newClass#integer> xsd:integer <http://example.org/newClass#i> .

    <http://example.org/newClass#string> xsd:string <http://example.org/newClass#s> .

    <http://example.org/newClass#uri> xsd:anyURI <http://example.org/newClass#u> .

    <http://example.org/newClass#i> rdf:value <http://example.org/newClass#newClass> .

    <http://example.org/newClass#s> rdf:value <http://example.org/newClass#newClass> .

    <http://example.org/newClass#u> rdf:value <http://example.org/newClass#newClass> .
}

root: {
    root: a rdfs:Class .

    <http://example.org/root#newClass> a <http://example.org/root#subsubclass> .

    <http://example.org/root#subclass> rdfs:member <http://example.org/root#root> .

    <http://example.org/root#subsubclass> rdfs:member <http://example.org/root#subclass> .
}

