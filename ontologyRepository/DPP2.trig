@prefix ADDRESS: <http://example.org/ADDRESS> .
@prefix PRODUCER: <http://example.org/PRODUCER> .
@prefix ROOT: <http://example.org/ROOT> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

PRODUCER: {
    PRODUCER: a rdfs:Class .

    <http://example.org/PRODUCER#PRODUCER> rdf:value <http://example.org/PRODUCER#name> .

    <http://example.org/PRODUCER#address> rdfs:isDefinedBy <http://example.org/PRODUCER#ADDRESS> ;
        rdfs:member PRODUCER: .

    <http://example.org/PRODUCER#name> rdfs:Datatype xsd:string .
}

ADDRESS: {
    ADDRESS: a rdfs:Class .

    <http://example.org/ADDRESS#ADDRESS> rdf:value <http://example.org/ADDRESS#address_line_1>,
            <http://example.org/ADDRESS#address_line_2>,
            <http://example.org/ADDRESS#zip> .

    <http://example.org/ADDRESS#address_line_1> rdfs:Datatype xsd:string .

    <http://example.org/ADDRESS#address_line_2> rdfs:Datatype xsd:string .

    <http://example.org/ADDRESS#zip> rdfs:Datatype xsd:integer .
}

ROOT: {
    ROOT: a rdfs:Class .

    <http://example.org/ROOT#producer> rdfs:isDefinedBy <http://example.org/ROOT#PRODUCER> ;
        rdfs:member <http://example.org/ROOT#product> .

    <http://example.org/ROOT#name> rdfs:Datatype xsd:string .

    <http://example.org/ROOT#product> rdf:value <http://example.org/ROOT#name> ;
        rdfs:member ROOT: .
}

