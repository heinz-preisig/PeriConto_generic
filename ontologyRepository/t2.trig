@prefix newClass: <http://example.org/newClass> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix root: <http://example.org/root> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

newClass: {
    newClass: a rdfs:Class .
}

root: {
    root: a rdfs:Class .

    <http://example.org/root#newClass> a <http://example.org/root#rootSubSub> .

    xsd:integer rdfs:Datatype <http://example.org/root#i> .

    <http://example.org/root#i> rdf:value <http://example.org/root#rootSub> .

    <http://example.org/root#rootSubSub> rdfs:member <http://example.org/root#rootSub> .

    <http://example.org/root#rootSub> rdfs:member <http://example.org/root#root> .
}

