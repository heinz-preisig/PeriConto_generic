@prefix o: <http://example.org/o> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix root: <http://example.org/root> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

o: {
    o: a rdfs:Class .

    <http://example.org/o#k> rdfs:subClassOf <http://example.org/o#o> .
}

root: {
    root: a rdfs:Class .

    <http://example.org/root#k> rdfs:subClassOf <http://example.org/root#root> .

    <http://example.org/root#string> xsd:string <http://example.org/root#pr> .

    <http://example.org/root#pr> rdf:value <http://example.org/root#root> .
}

