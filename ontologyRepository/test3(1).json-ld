@prefix ROOT: <http://example.org/ROOT> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ROOT: {
    ROOT: a rdfs:Class ;
        rdfs:Datatype <http://example.org/ROOT#elucidation> .

    <http://example.org/ROOT#item1> rdf:value <http://example.org/ROOT#name> ;
        rdfs:Datatype <http://example.org/ROOT#elucidation> ;
        rdfs:member ROOT: .

    <http://example.org/ROOT#name> rdfs:Datatype xsd:string .

    <http://example.org/ROOT#elucidation> rdf:value rdfs:comment .
}

