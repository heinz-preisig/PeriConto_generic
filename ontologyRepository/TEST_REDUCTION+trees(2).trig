@prefix GUGUS: <http://example.org/GUGUS#> .
@prefix GUGUS_i: <http://example.org/GUGUS_i#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

GUGUS:GUGUS {
    GUGUS:GUGUS a rdfs:Class .

    GUGUS:Link rdfs:member GUGUS:GUGUS .

    "True" xsd:boolean GUGUS:HelloMe .

    GUGUS:HelloMe rdf:value GUGUS:GUGUS .
}

GUGUS_i:GUGUS_i {
    GUGUS_i:GUGUS_i a rdfs:Class .

    GUGUS_i:HelloMe rdf:value GUGUS_i:GUGUS_i .

    "True" xsd:boolean GUGUS_i:HelloMe .
}

