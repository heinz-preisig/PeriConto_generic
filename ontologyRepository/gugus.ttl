@prefix ckg: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ckg:AddressSpecifications rdf:value ckg:AddressLine_01,
        ckg:AddressLine_02,
        ckg:City,
        ckg:Country,
        ckg:ZIP ;
    rdfs:isDefinedBy ckg:Address .

ckg:Address rdfs:subClassOf ckg:ProducerSpecifications .

ckg:AddressLine_01 xsd:string ckg:string .

ckg:AddressLine_02 xsd:string ckg:string .

ckg:City xsd:string ckg:string .

ckg:Country xsd:string ckg:string .

ckg:OranisationName xsd:string ckg:string .

ckg:OranisationNumber xsd:integer ckg:integer .

ckg:Producer rdfs:subClassOf ckg:ProductSpecifications .

ckg:ProducerSpecifications rdf:value ckg:OranisationName,
        ckg:OranisationNumber ;
    rdfs:isDefinedBy ckg:Producer .

ckg:Product rdfs:subClassOf ckg:PassPort .

ckg:ProductSpecifications rdfs:isDefinedBy ckg:Product,
        ckg:rawProduct_01,
        ckg:rawProduct_02,
        ckg:rawProduct_03 .

ckg:ZIP xsd:integer ckg:integer .

ckg:rawProduct_01 rdfs:subClassOf ckg:PassPort .

ckg:rawProduct_02 rdfs:subClassOf ckg:PassPort .

ckg:rawProduct_03 rdfs:subClassOf ckg:PassPort .

